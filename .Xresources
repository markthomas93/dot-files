! contrast color scheme for X

#define S_white         #c0c0c0
#define S_red           #cf0000
#define S_yellow        #cfcf00
#define S_green         #00cf00
#define S_blue          #0000cf
#define S_cyan          #00cfcf
#define S_magenta       #cf00cf
#define S_black         #000000

#define S_lightwhite    #f9f9f9
#define S_lightred      #ff0000
#define S_lightyellow   #ffff00
#define S_lightgreen    #00ff00
#define S_lightblue     #0000ff
#define S_lightcyan     #00ffff
#define S_lightmagenta  #ff00ff
#define S_lightblack    #1c1c1c

#define S_lightgray     #afafaf
#define S_medgray       #686868
#define S_darkgray      #262626

! xft

xft*antialias:         true
xft*autohint:          false
xft*dpi:               140
xft*rgba:              rgb
xft*hinting:           true
xft*hintStyle:         hintfull
xft*lcdfilter:         lcddefault

! xst

st*shell:                     /bin/zsh

st*termname:                   st-24bit
st*borderpx:                   0

st*foreground:                #f9f9f9
st*background:                #262626

st*font:                       Droid Sans Mono Slashed:style=Regular:size=8:anitalias=true:autohint=true

st*color0:       #1e1e1e
st*color1:       #cf6a4c
st*color2:       #8f9d6a
st*color3:       #f9ee98
st*color4:       #7587a6
st*color5:       #9b859d
st*color6:       #afc4db
st*color7:       #a7a7a7
st*color8:       #5f5a60
st*color9:       #cf6a4c
st*color10:      #8f9d6a
st*color11:      #f9ee98
st*color12:      #7587a6
st*color13:      #9b859d
st*color14:      #afc4db
st*color15:      #ffffff
st*color16:      #cda869
st*color17:      #9b703f
st*color18:      #323537
st*color19:      #464b50
st*color20:      #838184
st*color21:      #c3c3c3

! xscreensaver

xscreensaver.splash:                    false

xscreensaver.Dialog.headingFont:        -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.Dialog.bodyFont:           -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.Dialog.labelFont:          -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.Dialog.unameFont:          -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.Dialog.buttonFont:         -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.Dialog.dateFont:           -*-clean-*-*-*-*-8-*-*-*-*-*-*-*

xscreensaver.Dialog.foreground:         S_medgray
xscreensaver.Dialog.background:         S_darkgray
xscreensaver.Dialog.topShadowColor:     S_darkgray
xscreensaver.Dialog.bottomShadowColor:  S_darkgray
xscreensaver.Dialog.Button.foreground:  S_medgray
xscreensaver.Dialog.Button.background:  S_darkgray
xscreensaver.Dialog.text.foreground:    S_medgray
xscreensaver.Dialog.text.background:    S_darkgray
xscreensaver.Dialog.internalBorderWidth:0
xscreensaver.Dialog.borderWidth:        0
xscreensaver.Dialog.shadowThickness:    2

xscreensaver.passwd.passwdFont:         -*-clean-*-*-*-*-8-*-*-*-*-*-*-*
xscreensaver.passwd.thermometer.foreground:  S_red
xscreensaver.passwd.thermometer.background:  S_darkgray
xscreensaver.passwd.thermometer.width:       0

xscreensaver.dateFormat:
