text/html; /usr/bin/w3m -I %{charset} -T text/html; copiousoutput
text/calendar; /usr/bin/gcalcli import -v %s; copiousoutput; description=Calendar Invite
application/pdf; /usr/bin/pdftotext %s -; copiousoutput; description=PDF Document; nametemplate=%s.pdf
application/msword; /usr/bin/antiword %s; copiousoutput; description=Word Document; nametemplate=%s.doc

